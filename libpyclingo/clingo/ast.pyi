class AST:
    def __init__(self, type: ASTType, **arguments: Mapping[str,Any]): ...
    def items(self) -> List[Tuple[str,AST]]: ...
    def keys(self) -> List[str]: ...
    def values(self) -> List[AST]: ...
    child_keys: List[str]
    type: ASTType

